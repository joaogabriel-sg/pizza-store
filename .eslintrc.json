{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-param-reassign": 0,
        "no-unused-vars": 0,
        "implicit-arrow-linebreak": 0
    }
}
